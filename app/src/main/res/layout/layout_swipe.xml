<?xml version="1.0" encoding="utf-8"?>
<com.jwen.expandrecycler.swipemenu.SwipeMenuLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:jwen="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    jwen:menuViewId="@id/swipe_menu"
    jwen:contentViewId="@id/swipe_content">
    <com.jwen.expandrecycler.swipemenu.SwipeMenuView
        android:id="@id/swipe_menu"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        />
    <FrameLayout
        android:id="@id/swipe_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
</com.jwen.expandrecycler.swipemenu.SwipeMenuLayout>
